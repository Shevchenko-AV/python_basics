    "import json\n",
    "\n",
    "purchases = {}\n",
    "with open('purchase_log.txt', 'r', encoding='utf-8') as f:\n",
    "  for i, line in enumerate(f):\n",
    "    if i == 0:\n",
    "      continue\n",
    "    purchase = json.loads(line.strip())\n",
    "    purchases[purchase['user_id']] = purchase['category']\n",
    "for key, value in purchases.items():\n",
    "  print(key, value)"
   ]
  }
